<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/nav_graph"
        app:startDestination="@id/transportsListFragment">

    <fragment
            android:id="@+id/transportsListFragment"
            android:name="hu.selester.selnet.fragments.TransportsListFragment"
            android:label="TransportsListFragment"
            tools:layout="@layout/frg_transports_list">
        <action
                android:id="@+id/action_transportsListFragment_to_signatureFragment"
                app:destination="@id/signatureFragment" />
        <action
                android:id="@+id/action_transportsListFragment_to_transDataFragment"
                app:destination="@id/transDataFragment" />
        <action
                android:id="@+id/action_transportsListFragment_to_transPhotoFragment"
                app:destination="@id/transPhotoFragment" />
        <action
                android:id="@+id/action_transportsListFragment_self"
                app:destination="@id/transportsListFragment" />
    </fragment>
    <fragment
            android:id="@+id/signatureFragment"
            android:name="hu.selester.selnet.fragments.SignatureFragment"
            android:label="SignatureFragment"
            tools:layout="@layout/dialog_signature"/>
    <fragment
            android:id="@+id/verifyLoginFragment"
            android:name="hu.selester.selnet.fragments.VerifyLoginFragment"
            android:label="VerifyLoginFragment"
            tools:layout="@layout/frg_verify_login">
        <action
                android:id="@+id/action_verifyLoginFragment_to_transportsListFragment"
                app:destination="@id/workDatasFragment"
                app:popUpTo="@id/nav_graph"
                app:popUpToInclusive="true" />
    </fragment>
    <fragment
            android:id="@+id/workDatasFragment"
            android:name="hu.selester.selnet.fragments.WorkDataFragment"
            android:label="WorkDatasFragment"
            tools:layout="@layout/frg_workdata">
        <action
                android:id="@+id/action_workDatasFragment_to_transportsListFragment"
                app:destination="@id/transportsListFragment" />
    </fragment>
    <fragment
            android:id="@+id/transDataFragment"
            android:name="hu.selester.selnet.fragments.TransDataFragment"
            android:label="TransDataFragment"
            tools:layout="@layout/frg_transdata"/>
    <fragment
            android:id="@+id/transPhotoFragment"
            android:name="hu.selester.selnet.fragments.TransPhotoFragment"
            android:label="TransPhotoFragment"
            tools:layout="@layout/frg_transphoto"/>
</navigation>